<HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="1200.0" styleClass="bg-white" stylesheets="@style/global.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.controllers.NavbarController">
   <children>
      <HBox prefHeight="56.0" prefWidth="217.0">
         <children>
            <ImageView fitHeight="56.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@assets/logo/EventLogo.png" />
               </image>
            </ImageView>
            <Label prefHeight="56.0" prefWidth="186.0" text="EVENT HUB">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Pane prefHeight="56.0" prefWidth="20.0" />
      <HBox alignment="CENTER_LEFT" fillHeight="false" maxHeight="-Infinity" prefHeight="72.0" prefWidth="355.0" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onHandleGoToHomePage" prefHeight="72.0" prefWidth="97.0" text="หน้าแรก">
               <styleClass>
                  <String fx:value="text-lg" />
                  <String fx:value="btn-transparent" />
               </styleClass>
            </Button>
            <HBox alignment="CENTER_LEFT" fillHeight="false" maxHeight="-Infinity" prefHeight="121.0" prefWidth="138.0">
               <children>
                  <Button mnemonicParsing="false" prefHeight="77.0" prefWidth="115.0" text="บริหารจัดการ">
                     <styleClass>
                        <String fx:value="btn-transparent" />
                        <String fx:value="text-lg" />
                     </styleClass>
                  </Button>
                  <ImageView fitHeight="14.0" fitWidth="108.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@assets/Icons/Vector.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="-3.0" left="-3.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#onHandleGoToContactUs" prefHeight="147.0" prefWidth="106.0" text="ติดต่อเรา">
               <styleClass>
                  <String fx:value="btn-transparent" />
                  <String fx:value="text-lg" />
               </styleClass>
            </Button>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
      <Pane prefHeight="56.0" prefWidth="311.0" />
      <HBox alignment="CENTER_LEFT" fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="56.0" prefWidth="307.0" spacing="15.0">
         <children>
            <ToggleButton mnemonicParsing="false" prefHeight="40.0" prefWidth="119.0" text="Dark">
               <styleClass>
                  <String fx:value="btn-danger" />
                  <String fx:value="text-lg" />
               </styleClass>
            </ToggleButton>
            <Circle fx:id="navImageProfile" fill="WHITE" radius="23.0" stroke="#462b63" strokeType="INSIDE" styleClass="image-border" />
            <VBox prefHeight="56.0" prefWidth="143.0">
               <children>
                  <Label prefHeight="40.0" prefWidth="137.0" text="กิตติคุณ">
                     <styleClass>
                        <String fx:value="text-xl" />
                        <String fx:value="font-bold" />
                     </styleClass>
                  </Label>
                  <Label alignment="TOP_LEFT" text="ผู้ดูแลระบบ">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <padding>
                        <Insets top="-13.0" />
                     </padding>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
